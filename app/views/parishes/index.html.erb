<p style="color: green"><%= notice %></p>

<h1 class="display-4">Parishes</h1>
<table class="table">
  <thead>
    <th>Parish</th>
    <th>More...</th>
  </thead>
<div id="parishes">
  <tr>
  <% @parishes.each do |parish| %>
    <td><%= parish.name %> </td>
    <td><%= link_to "Show this parish", parish %> </td>
    </tr>
  <% end %>
</table>
  <%= link_to "New Parish", new_parish_path, class: "btn btn-success" %>
</div>


